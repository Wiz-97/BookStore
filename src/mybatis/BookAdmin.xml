<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bookadmin">  
  	<select id="getAll" resultType="BookAdmin">
  		select * from pro_booklist order by bno desc
  	</select>
  	<insert id="insert" parameterType="BookDao">
  		insert into pro_booklist(bno,title,b_img) values(gal_idx_seq.nextval, #{title},#{b_img})
  	</insert>
  	<select id="selectOne" parameterType="int" resultType="BookDao">
  		select * from pro_booklist where bno = #{bno}
  	</select>
</mapper>